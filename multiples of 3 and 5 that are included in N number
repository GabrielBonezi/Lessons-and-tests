function solution(number){
  console.log(number);  
  let arr = [];
    for (let i =0; i<number; i++){
        let x = i/5;
        let y = i/3;
        if (Number.isInteger(y) && y>0){
        arr.push(i);
      }else if 
      (Number.isInteger(x) && x>0){
          arr.push(i);
      }
      
    }
       let sum = 0
    for (let t=0; t<arr.length;t++){
        sum += arr[t]
    }
    
    console.log(arr);
  return sum;
}
solution(10); //output 23
